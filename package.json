{
  "name": "finance-tracker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf node_modules .output dist pnpm-lock.yaml",
    "db:generate": "pnpm with:env drizzle-kit generate",
    "db:migrate": "pnpm with:env drizzle-kit migrate",
    "db:pull": "pnpm with:env drizzle-kit pull",
    "db:push": "pnpm with:env drizzle-kit push",
    "db:studio": "pnpm with:env drizzle-kit studio",
    "dev": "vite dev",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f db",
    "docker:reset": "docker compose down -v",
    "docker:up": "docker compose up -d",
    "format": "prettier -uwl --cache .",
    "format:check": "prettier -uc --cache .",
    "lint": "eslint --max-warnings 0 --cache --cache-location node_modules/.cache/eslint/.eslintcache .",
    "lint:fix": "eslint --max-warnings 0 --fix --cache --cache-location node_modules/.cache/eslint/.eslintcache .",
    "lint:md": "markdownlint-cli2 '**/*.md'",
    "lint:md:fix": "markdownlint-cli2 --fix '**/*.md'",
    "paraglide:compile": "pnpm exec paraglide-js compile --project ./project.inlang --outdir ./src/paraglide",
    "preview": "vite preview",
    "schema:auth": "pnpm schema:auth:generate && pnpm schema:auth:postprocess",
    "schema:auth:generate": "pnpm with:env pnpm dlx @better-auth/cli generate --config src/lib/auth.ts --output src/modules/auth/schema.ts --yes",
    "schema:auth:postprocess": "tsx scripts/postprocess-auth-schema.ts",
    "start": "pnpm with:env node .output/server/index.mjs",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "with:env": "dotenvx run --"
  },
  "dependencies": {
